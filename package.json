{
  "name": "@vue-dnd-kit/core",
  "version": "0.0.15",
  "description": "Core functionality for Vue DnD Kit - a lightweight Vue 3 library for building performant and accessible drag and drop interfaces",
  "author": "ZiZIGY",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zizigy/vue-dnd-kit.git",
    "directory": "packages/core"
  },
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.vue",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.38.0",
    "typescript": "^4.9.5",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
